<script>
    var api = new XMLHttpRequest();
    api.open("GET", "/Home/GetPagesByRol/" + @ViewBag.User.id_rol);
    api.send();

    api.onreadystatechange = function() {
        if (this.status == 200 && this.readyState == 4) {

            debugger;

            let data = JSON.parse(this.responseText);

            data.forEach((item) => {
                document.getElementById(item.page).removeAttribute("hidden");
            });
        }
    }
</script>